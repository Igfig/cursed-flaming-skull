{
	"name": "cursed-flaming-skull",
	"version": "0.0.0",
	"description": "A tabletop game design blog, built with Eleventy",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"serve": "npx @11ty/eleventy --serve",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Ira Fich",
		"email": "ira.fich@gmail.com",
		"url": "https://irafich.com/"
	},
	"license": "All rights reserved",
	"engines": {
		"node": ">=18"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"cross-env": "^7.0.3",
		"eleventy-plugin-blog-tools": "^0.4.1",
		"luxon": "^3.6.1",
		"prismjs": "^1.30.0",
		"sass": "^1.96.0",
		"zod": "^3.25.67",
		"zod-validation-error": "^3.5.2"
	},
	"dependencies": {
		"@11ty/font-awesome": "github:11ty/eleventy-plugin-font-awesome",
		"@fortawesome/free-brands-svg-icons": "^7.1.0",
		"@zachleat/heading-anchors": "^1.0.5"
	}
}
